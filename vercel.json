{
    "builds": [
        {
            "src": "**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "**/*.js",
            "use": "@vercel/static"
        },
        {
            "src": "**/*.css",
            "use": "@vercel/static"
        },
        {
            "src": "**/*.{png,jpg,gif}",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "buildCommand": "sed -i 's/__KAKAO_MAP_API_KEY__/'\"$KAKAO_MAP_API_KEY\"'/g; s#__BACKEND_API_URL__#'\"$BACKEND_API_URL\"'#g' src/js/config.js"
}